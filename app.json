{
  "expo": {
    "name": "EgTake",
    "slug": "test-eas-upload",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/EgtakeLogo.png",
    "scheme": "testeasupload",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/EgtakeLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.emamexp2.testeasupload",
      "permissions": [
        "CAMERA"
      ],
      "blockedPermissions": [
       "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK"
     ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/EgtakeLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/EgtakeLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d1205969-c5d0-4608-9c6f-ca3f8da33306"
      }
    },
    "owner": "emamexp2"
  }
}
